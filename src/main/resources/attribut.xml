<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="appLookAndFeel" class="java.lang.String">
        <constructor-arg value="${app.LOOK_AND_FEEL}"/>
    </bean>
    <bean id="appTitle" class="java.lang.String">
        <constructor-arg value="${app.TITLE}"/>
    </bean>
    <bean id="appVersion" class="java.lang.String">
        <constructor-arg value="${app.VERSION}"/>
    </bean>
    <bean id="appDescription" class="java.lang.String">
        <constructor-arg value="${app.DESCRIPTION}"/>
    </bean>
    <bean id="appAuthor" class="java.lang.String">
        <constructor-arg value="${app.AUTHOR}"/>
    </bean>
</beans>